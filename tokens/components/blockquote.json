{
  "components/blockquote": {
    "utrecht": {
      "blockquote": {
        "background-color": {
          "$type": "color",
          "$value": "{basis.color.transparent}"
        },
        "border-color": {
          "$type": "color",
          "$value": "{basis.color.transparent}"
        },
        "border-inline-start-width": {
          "$type": "dimension",
          "$value": "0px"
        },
        "border-radius": {
          "$type": "dimension",
          "$value": "0px"
        },
        "border-width": {
          "$type": "dimension",
          "$value": "0px"
        },
        "color": {
          "$type": "color",
          "$value": "{basis.color.default.color-document}"
        },
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{basis.text.font-family.default}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{basis.text.font-size-X.md}"
        },
        "padding-block-end": {
          "$type": "dimension",
          "$value": "{basis.space.block.lg}"
        },
        "padding-block-start": {
          "$type": "dimension",
          "$value": "{basis.space.block.lg}"
        },
        "padding-inline-end": {
          "$type": "dimension",
          "$value": "0px"
        },
        "padding-inline-start": {
          "$type": "dimension",
          "$value": "0px"
        },
        "row-gap": {
          "$type": "dimension",
          "$value": "{basis.space.row.md}"
        },
        "attribution": {
          "color": {
            "$type": "color",
            "$value": "{basis.color.default.color-subtle}"
          },
          "font-size": {
            "$type": "fontSizes",
            "$value": "{basis.text.font-size-X.md}"
          }
        },
        "content": {
          "color": {
            "$type": "color",
            "$value": "{basis.color.default.color-document}"
          },
          "font-size": {
            "$type": "fontSizes",
            "$value": "{basis.text.font-size-X.xl}"
          }
        }
      }
    }
  }
}