          "line-height": {
            "$type": "lineHeights",
            "$value": "{basis.text.line-height-X.lg}"
          },
          "margin-block-end": {
            "$type": "dimension",
            "$value": "0px",
            "$description": "[code-only]"
          },
          "margin-block-start": {
            "$type": "dimension",
            "$value": "0px",
            "$description": "[code-only]"
          }
