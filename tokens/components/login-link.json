{
  "components/login-link": {
    "lux": {
      "login-link": {
        "background-color": {
          "$type": "color",
          "$value": "{basis.color.transparent}"
        },
        "border-color": {
          "$type": "color",
          "$value": "{basis.color.default.border-default}"
        },
        "border-radius": {
          "$type": "dimension",
          "$value": "{basis.border-radius.md}"
        },
        "border-width": {
          "$type": "dimension",
          "$value": "{basis.border-width.sm}"
        },
        "color": {
          "$type": "color",
          "$value": "{basis.color.action-2.color-default}"
        },
        "column-gap": {
          "$type": "dimension",
          "$value": "{basis.space.text.lg}"
        },
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{basis.text.font-family.default}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{basis.text.font-size-X.md}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{basis.text.font-weight.bold}"
        },
        "line-height": {
          "$type": "lineHeights",
          "$value": "{basis.text.line-height-X.md}"
        },
        "min-block-size": {
          "$type": "dimension",
          "$value": "{basis.pointer-target.min-block-size}"
        },
        "min-inline-size": {
          "$type": "dimension",
          "$value": "{basis.pointer-target.min-inline-size}"
        },
        "padding-block-end": {
          "$type": "dimension",
          "$value": "{basis.space.block.md}"
        },
        "padding-block-start": {
          "$type": "dimension",
          "$value": "{basis.space.block.md}"
        },
        "padding-inline-end": {
          "$type": "dimension",
          "$value": "{basis.space.inline.md}"
        },
        "padding-inline-start": {
          "$type": "dimension",
          "$value": "{basis.space.inline.md}"
        },
        "icon": {
          "size": {
            "$type": "dimension",
            "$value": "{basis.size.icon.4xl}"
          }
        },
        "active": {
          "background-color": {
            "$type": "color",
            "$value": "{basis.color.default.bg-active}"
          },
          "border-color": {
            "$type": "color",
            "$value": "{basis.color.default.border-active}"
          },
          "color": {
            "$type": "color",
            "$value": "{basis.color.action-2.color-active}"
          }
        },
        "hover": {
          "background-color": {
            "$type": "color",
            "$value": "{basis.color.default.bg-hover}"
          },
          "border-color": {
            "$type": "color",
            "$value": "{basis.color.default.border-hover}"
          },
          "color": {
            "$type": "color",
            "$value": "{basis.color.action-2.color-hover}"
          }
        }
      }
    },
    "todo": {
      "login-link": {
        "focus-visible": {
          "background-color": {
            "$type": "color",
            "$value": "{basis.focus.background-color}"
          },
          "border-color": {
            "$type": "color",
            "$value": "{basis.color.transparent}"
          },
          "color": {
            "$type": "color",
            "$value": "{basis.focus.color}"
          }
        }
      }
    }
