{
  "components/form-field": {
    "utrecht": {
      "form-field": {
        "invalid": {
          "border-inline-start-color": {
            "$type": "color",
            "$value": "{basis.color.negative.border-default}"
          },
          "border-inline-start-width": {
            "$type": "dimension",
            "$value": "{basis.border-width.md}"
          },
          "padding-inline-start": {
            "$type": "dimension",
            "$value": "{basis.space.inline.xl}"
          }
        }
      }
    }
  }
}