{
  "components/form-field-label/todo": {
    "todo": {
      "form-field-label": {
        "color": {
          "$type": "color",
          "$value": "{basis.color.default.color-document}"
        },
        "column-gap": {
          "$type": "dimension",
          "$value": "{basis.space.text.md}"
        },
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{basis.text.font-family.default}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{basis.text.font-size-X.md}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{basis.text.font-weight.bold}"
        },
        "line-height": {
          "$type": "lineHeights",
          "$value": "{basis.text.line-height-X.md}"
        }
      }
    }
  }
}