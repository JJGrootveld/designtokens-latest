{
  "components/form-field-label/utrecht": {
    "utrecht": {
      "form-label": {
        "color": {
          "$type": "color",
          "$value": "{basis.color.default.color-document}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{basis.text.font-size-X.md}"
        },
        "font-weight": {
          "$type": "fontWeights",
          "$value": "{basis.text.font-weight.bold}"
        },
        "checkbox": {
          "color": {
            "$type": "color",
            "$value": "{basis.color.default.color-document}"
          },
          "font-weight": {
            "$type": "fontWeights",
            "$value": "{basis.text.font-weight.default}"
          }
        },
        "checked": {
          "font-weight": {
            "$type": "fontWeights",
            "$value": "{basis.text.font-weight.default}"
          }
        },
        "disabled": {
          "color": {
            "$type": "color",
            "$value": "{basis.color.disabled.color-subtle}"
          }
        },
        "radio": {
          "color": {
            "$type": "color",
            "$value": "{basis.color.default.color-document}"
          },
          "font-weight": {
            "$type": "fontWeights",
            "$value": "{basis.text.font-weight.default}"
          }
        }
      }
    }
  }
}