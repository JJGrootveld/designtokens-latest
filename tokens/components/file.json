{
  "components/file": {
    "denhaag": {
      "file": {
        "border-color": {
          "$type": "color",
          "$value": "{basis.color.default.border-subtle}"
        },
        "border-width": {
          "$type": "dimension",
          "$value": "{basis.border-width.sm}"
        },
        "border-style": {
          "$type": "other",
          "$value": "solid"
        },
        "left": {
          "background-color": {
            "$type": "color",
            "$value": "{basis.color.default.bg-default}"
          },
          "padding-inline-end": {
            "$type": "dimension",
            "$value": "{basis.space.inline.xl}"
          },
          "padding-inline-start": {
            "$type": "dimension",
            "$value": "{basis.space.inline.xl}"
          }
        },
        "link": {
          "color": {
            "$type": "color",
            "$value": "{basis.color.action-2.color-default}"
          },
          "gap": {
            "$type": "dimension",
            "$value": "{basis.space.text.xs}"
          },
          "icon": {
            "width": {
              "$type": "dimension",
              "$value": "{basis.size.icon.md}"
            }
          },
          "hover": {
            "color": {
              "$type": "color",
              "$value": "{basis.color.action-2.color-hover}"
            }
          }
        },
        "right": {
          "gap": {
            "$type": "dimension",
            "$value": "{basis.space.row.md}"
          },
          "padding-block-end": {
            "$type": "dimension",
            "$value": "{basis.space.block.lg}"
          },
          "padding-block-start": {
            "$type": "dimension",
            "$value": "{basis.space.block.lg}"
          },
          "padding-inline-end": {
            "$type": "dimension",
            "$value": "{basis.space.inline.xl}"
          },
          "padding-inline-start": {
            "$type": "dimension",
            "$value": "{basis.space.inline.xl}"
          }
        }
      }
    }
  }
}