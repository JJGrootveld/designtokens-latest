{
  "components/breadcrumb-nav/utrecht": {
    "utrecht": {
      "breadcrumb-nav": {
        "font-family": {
          "$type": "fontFamilies",
          "$value": "{basis.text.font-family.default}"
        },
        "font-size": {
          "$type": "fontSizes",
          "$value": "{basis.text.font-size-X.md}"
        },
        "line-height": {
          "$type": "lineHeights",
          "$value": "{basis.text.line-height-X.md}"
        },
        "min-block-size": {
          "$type": "dimension",
          "$value": "{basis.pointer-target.min-block-size}"
        },
        "item": {
          "padding-block-end": {
            "$type": "dimension",
            "$value": "{basis.space.block.lg}"
          },
          "padding-block-start": {
            "$type": "dimension",
            "$value": "{basis.space.block.lg}"
          },
          "padding-inline-end": {
            "$type": "dimension",
            "$value": "{basis.space.inline.md}"
          },
          "padding-inline-start": {
            "$type": "dimension",
            "$value": "{basis.space.inline.md}"
          },
          "first": {
            "padding-inline-start": {
              "$type": "dimension",
              "$value": "0px"
            }
          }
        },
        "link": {
          "color": {
            "$type": "color",
            "$value": "{basis.color.action-2.color-default}"
          },
          "text-decoration": {
            "$type": "textDecoration",
            "$value": "underline"
          },
          "active": {
            "color": {
              "$type": "color",
              "$value": "{basis.color.action-2.color-active}"
            },
            "text-decoration": {
              "$type": "textDecoration",
              "$value": "None"
            }
          },
          "current": {
            "font-weight": {
              "$type": "fontWeights",
              "$value": "{basis.text.font-weight.default}"
            }
          },
          "disabled": {
            "color": {
              "$type": "color",
              "$value": "{basis.color.disabled.color-subtle}"
            }
          },
          "focus": {
            "background-color": {
              "$type": "color",
              "$value": "{basis.focus.background-color}"
            },
            "color": {
              "$type": "color",
              "$value": "{basis.focus.color}"
            },
            "text-decoration": {
              "$type": "textDecoration",
              "$value": "None"
            }
          },
          "hover": {
            "color": {
              "$type": "color",
              "$value": "{basis.color.action-2.color-hover}"
            },
            "text-decoration": {
              "$type": "textDecoration",
              "$value": "None"
            }
          }
        },
        "separator": {
          "color": {
            "$type": "color",
            "$value": "{basis.color.default.color-subtle}"
          },
          "icon": {
            "size": {
              "$type": "dimension",
              "$value": "{basis.size.icon.sm}"
            }
          }
        }
      }
    }
  }
}